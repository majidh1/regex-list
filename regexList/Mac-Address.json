        "AA:BB:CC:DD:EE:FF",
        "4A:1B:CC:D5:EE:F2",
        "AABBCCDDEEFF",
        "4AB2C9D7EE1F"
    ],
    "rejectTest": [
        "AA:BB:CC:DD:EE:FF-",
        "AA:BB:CC:DD:EE:FF:",
        "AA-BB-CC-DD-EE-FF",
        "AA:BB-CC:DD-EE:FF",
        "AA-BB-CC-DD-EE-GG",
        "AAGG",
        "AA1111GG",
        "AA:GG",
        "AA.BB.CC.DD.EE.FF"
    ]
}
